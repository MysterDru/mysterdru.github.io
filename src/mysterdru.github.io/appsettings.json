{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

    "SassCompiler": {
      "Source": "Styles",
      "Target": "wwwroot\\_d\\css",
      "Arguments": "--style=compressed",
      "GenerateScopedCss": true,
      "ScopedCssFolders": ["Views", "Pages", "Shared", "Components"]
    }
}
